    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.5.1",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.2.1"
  }
