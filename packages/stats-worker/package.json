    "@hop-protocol/sdk": "0.0.1-beta.344",
    "comment-json": "4.2.2",
    "csv-writer": "1.6.0",
    "dotenv": "10.0.0",
    "ethereum-block-by-date": "1.4.5",
    "ethers": "5.6.9",
    "luxon": "2.5.0",
    "minimist": "1.2.6",
    "sqlite3": "5.0.11",
    "typescript": "4.4.2",
    "uuid": "8.3.2",
    "wait": "0.4.2"
  },
  "devDependencies": {
    "@types/lodash": "4.14.178",
